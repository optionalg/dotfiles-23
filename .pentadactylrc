set guioptions=bCrsB
set hintkeys="aoeuidhtns"
set altwildmode=list:longest,full
set complete=lS
set hlfind

noremap  j 4j
noremap  k 4k
noremap  <C-g> <Esc>
vnoremap <C-g> <Esc>
inoremap <C-g> <Esc>
tnoremap <C-g> <Esc>
cnoremap <C-g> <Esc>
cnoremap <M-n> <C-n>
cnoremap <M-p> <C-p>
cnoremap <C-n> <Tab>
cnoremap <C-p> <S-Tab>
unmap <C-;>
